{
    "$schema": "http://json-schema.org/draft-04/schema",
    "id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "additionalProperties": true,
    "properties": {
        "logonID": {
            "id": "#/properties/logonID",
            "type": "string",
